@startuml
!include style.puml
skinparam ArrowFontStyle plain

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":AddressBookParser" as AddressBookParser LOGIC_COLOR
participant ":DeleteClientProfileCommandParser" as DeleteClientProfileCommandParser LOGIC_COLOR
participant "d:DeleteClientProfile" as DeleteClientProfile LOGIC_COLOR
participant "r:CommandResult" as CommandResult LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant "m:Model" as Model MODEL_COLOR
end box

[-> LogicManager : execute("delete n/Alice")
activate LogicManager

LogicManager -> AddressBookParser : parseCommand("delete n/Alice")
activate AddressBookParser

create DeleteClientProfileCommandParser
AddressBookParser -> DeleteClientProfileCommandParser
activate DeleteClientProfileCommandParser

DeleteClientProfileCommandParser --> AddressBookParser
deactivate DeleteClientProfileCommandParser

AddressBookParser -> DeleteClientProfileCommandParser : parse("n/Alice")
activate DeleteClientProfileCommandParser

create DeleteClientProfile
DeleteClientProfileCommandParser -> DeleteClientProfile
activate DeleteClientProfile

DeleteClientProfile --> DeleteClientProfileCommandParser :
deactivate DeleteClientProfile

DeleteClientProfileCommandParser --> AddressBookParser : d
deactivate DeleteClientProfileCommandParser
'Hidden arrow to position the destroy marker below the end of the activation bar.
DeleteClientProfileCommandParser -[hidden]-> AddressBookParser
destroy DeleteClientProfileCommandParser

AddressBookParser --> LogicManager : d
deactivate AddressBookParser

LogicManager -> DeleteClientProfile : execute(m)
activate DeleteClientProfile

DeleteClientProfile -> Model : deletePerson(Alice)
activate Model

Model --> DeleteClientProfile
deactivate Model

create CommandResult
DeleteClientProfile -> CommandResult
activate CommandResult

CommandResult --> DeleteClientProfile
deactivate CommandResult

DeleteClientProfile --> LogicManager : r
deactivate DeleteClientProfile

[<--LogicManager
deactivate LogicManager
@enduml
